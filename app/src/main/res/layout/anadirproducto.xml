<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/editCantidad"
        android:layout_width="317dp"
        android:layout_height="53dp"
        android:text="@string/stock"
        app:layout_constraintBottom_toTopOf="@+id/editCantidadMin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/botonFoto"
        app:layout_constraintTop_toBottomOf="@+id/editNombre" />

    <EditText
        android:id="@+id/editCantidadMin"
        android:layout_width="317dp"
        android:layout_height="53dp"
        android:layout_marginStart="112dp"
        android:text="@string/stockMin"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.266"
        app:layout_constraintStart_toEndOf="@+id/botonFoto"
        app:layout_constraintTop_toBottomOf="@+id/editCantidad" />

    <EditText
        android:id="@+id/textView11"
        android:layout_width="317dp"
        android:layout_height="53dp"
        android:layout_marginStart="112dp"
        android:text="@string/preciou"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.266"
        app:layout_constraintStart_toEndOf="@+id/botonFoto"
        app:layout_constraintTop_toBottomOf="@+id/editCantidadMin" />

    <ImageButton
        android:id="@+id/botonFoto"
        android:layout_width="435dp"
        android:layout_height="420dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/editCantidad"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.602"
        app:srcCompat="@drawable/masomenos" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="317dp"
        android:layout_height="53dp"
        android:text="@string/nuevo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.093" />

    <EditText
        android:id="@+id/editNombre"
        android:layout_width="317dp"
        android:layout_height="53dp"
        android:layout_marginStart="112dp"
        android:text="Nombre"
        app:layout_constraintBottom_toTopOf="@+id/editCantidad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.266"
        app:layout_constraintStart_toEndOf="@+id/botonFoto"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

</androidx.constraintlayout.widget.ConstraintLayout>